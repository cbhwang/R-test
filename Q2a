
m <- c("Placebo only", "Nicotene patch", "Zyban", "Zyban & Nicotene patch")
x <- c(30,52,85,95)
n <- c(160,244,244,245)
wl <- c(1,2,3,4)
wu <- c(1,2,3,4)

for (i=1;i=4;i++){
#f=binom.test(x[1],n[1])
f <- binomci(x=x[1],n=n[1],method=m[1])
wl[1]=f[2]
wu[1]=f[3]

xy <- data.frame(teatment=m, x=x, n=n, wald.lci=wl, wald.uci=wu)
print(xy)
